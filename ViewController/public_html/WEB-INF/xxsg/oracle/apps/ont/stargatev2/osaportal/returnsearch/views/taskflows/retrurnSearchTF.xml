      <input-parameter id="__7">
        <name>param_account_number</name>
        <value></value>
      </input-parameter>
      <input-parameter-map>#{pageFlowScope.SearchResultsMB.returnSearchParamMap}</input-parameter-map>
    </task-flow-call>
    <task-flow-call id="CommonApplicationTF">
      <task-flow-reference>
        <document>/WEB-INF/xxsg/oracle/apps/ont/stargatev2/osaportal/commonapplication/view/taskflows/CommonApplicationTF.xml</document>
        <id>CommonApplicationTF</id>
      </task-flow-reference>
      <input-parameter id="__9">
        <name>param_task_type</name>
        <value>OrderCancellation</value>
      </input-parameter>
      <input-parameter id="__10">
        <name>param_header_id</name>
        <value>#{pageFlowScope.ItemOperations.param_header_id}</value>
      </input-parameter>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <control-flow-rule id="__2">
      <from-activity-id>init</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>init</from-outcome>
        <to-activity-id>returnSearch</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>returnSearch</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>callCust</from-outcome>
        <to-activity-id>CustomerDetailTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>CommonApplicationTF</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
